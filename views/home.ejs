            <script>
                document.addEventListener('DOMContentLoaded', () => {
                    const cancelButtons = document.querySelectorAll('.cancel-button');
            
                    cancelButtons.forEach(button => {
                        button.addEventListener('click', async () => {
                            const appointmentId = button.getAttribute('data-id');
            
                            const result = await Swal.fire({
                                text: "Are you sure to cancel Appointment?",
                                icon: 'warning',
                                showCancelButton: true,
                                confirmButtonColor: '#3085d6',
                                cancelButtonColor: '#d33',
                                confirmButtonText: 'Yes, cancel it!',
                                cancelButtonText: 'No, keep it'
                            });
            
                            if (result.isConfirmed) {
                                try {
                                    const response = await fetch(`/cancelappointment/${appointmentId}`, {
                                        method: 'DELETE'
                                    });
            
                                    if (response.ok) {
                                        Swal.fire({
                                            icon: 'success',
                                            title: 'Cancelled!',
                                            text: 'Your appointment has been cancelled.',
                                            showConfirmButton: false,
                                            timer: 1500
                                        });
                                        // Optionally, remove the appointment row from the table
                                        button.closest('tr').remove();
                                        // Reload the page after cancellation
                                        window.location.reload();
                                    } else {
                                        Swal.fire({
                                            icon: 'error',
                                            title: 'Failed to Cancel',
                                            text: 'Failed to cancel the appointment. Please try again.',
                                        });
                                    }
                                } catch (error) {
                                    console.error('Error:', error);
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Error',
                                        text: 'An error occurred while cancelling the appointment.',
                                    });
                                }
                            }
                        });
                    });
                });
            </script>
            
            
            
            
