                                        <input type="text" class="form-control me-2" id="time_schedule_start" name="time_schedule_start" required step="3600">
                                        <span class="mx-2">to</span>
                                        <input type="text" class="form-control ms-2" id="time_schedule_end" name="time_schedule_end" required step="3600">
                                    </div>
                                </div>
                                
                                </div>
                    
                           
                    
                                <button type="submit"  href=/admin/addoctorpost class="btn btn-primary">Submit</button>
                            </div>
                        </div>
